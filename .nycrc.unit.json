{
  "all": true,
  "check-coverage": true,
  "reporter": [
    "html",
    "json",
    "lcov",
    "text",
    "text-summary"
  ],
  "reports-dir": "coverage-unit",
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "**/*.spec.js",
    "src/index.js",
    "src/server.js",
    "src/www/bootstrap/*"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 90,
  "statements": 80
}
